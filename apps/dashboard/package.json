{
  "name": "@congress/dashboard",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "pnpm with-env vite dev",
    "dev:i18n:extract": "i18next-cli extract --watch",
    "dev:i18n:types": "i18next-cli types --watch",
    "build": "vite build",
    "start": "vite start",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path .prettierignore",
    "lint": "eslint --flag unstable_native_nodejs_ts_config",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@congress/api": "workspace:*",
    "@congress/auth": "workspace:*",
    "@congress/db": "workspace:*",
    "@congress/ui": "workspace:*",
    "@fontsource-variable/rubik": "^5.2.8",
    "@t3-oss/env-core": "^0.13.8",
    "@tanstack/react-form": "catalog:",
    "@tanstack/react-query": "catalog:",
    "@tanstack/react-router": "^1.133.28",
    "@tanstack/react-router-devtools": "^1.133.28",
    "@tanstack/react-router-ssr-query": "^1.133.28",
    "@tanstack/react-start": "^1.133.29",
    "@trpc/client": "catalog:",
    "@trpc/server": "catalog:",
    "@trpc/tanstack-react-query": "catalog:",
    "better-auth": "catalog:",
    "i18next": "catalog:",
    "i18next-browser-languagedetector": "catalog:",
    "nitro": "npm:nitro-nightly@latest",
    "react": "catalog:react19",
    "react-dom": "catalog:react19",
    "react-i18next": "catalog:",
    "superjson": "2.2.3",
    "zod": "catalog:"
