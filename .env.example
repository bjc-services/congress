# @see https://www.better-auth.com/docs/installation
AUTH_SECRET=

LEGACY_DB_HOST=
LEGACY_DB_USERNAME=
LEGACY_DB_PASSWORD=
LEGACY_DB_DATABASE=

AWS_REGION=
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_S3_BUCKET=

TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_PHONE_NUMBER=