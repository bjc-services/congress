{
  "name": "@congress/s3",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./env": {
      "types": "./dist/env.d.ts",
      "default": "./env.ts"
    }
  },
  "scripts": {
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "generate": "dotenv -e ../../.env -- pnpx @better-auth/cli generate --config script/auth-cli.ts --output ../db/src/auth-schema.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.929.0",
    "@aws-sdk/s3-presigned-post": "^3.929.0",
    "@aws-sdk/s3-request-presigner": "^3.929.0",
    "@congress/db": "workspace:*",
    "@t3-oss/env-core": "^0.13.8"
  },
  "devDependencies": {
    "@congress/eslint-config": "workspace:*",
    "@congress/prettier-config": "workspace:*",
    "@congress/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@congress/prettier-config"
}
